{
  "name": "@devprotocol/hashi",
  "version": "0.1.0-alpha",
  "description": "Dev Protocol's design system implementation for the web.",
  "main": "index.js",
  "repository": "https://github.com/dev-protocol/hashi",
  "author": "Dev Protocol",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build:ts": "webpack --config webpack.config.js",
    "build:ts:watch": "webpack --config webpack.config.js --watch",
    "build:sass": "gulp",
    "publish": "cd src && npm publish --access=public",
    "bump": "npm --no-git-tag-version version patch && cd src && npm --no-git-tag-version version patch"
  },
  "dependencies": {
    "@matteusan/sentro": "0.1.7"
  },
  "devDependencies": {
    "gulp": "4.0.2",
    "gulp-cli": "2.3.0",
    "gulp-purgecss": "4.0.3",
    "gulp-sass": "5.0.0",
    "sass": "1.43.5",
    "ts-loader": "9.2.6",
    "typescript": "4.5.2",
    "webpack": "5.64.4",
    "webpack-cli": "4.9.1"
  }
}
